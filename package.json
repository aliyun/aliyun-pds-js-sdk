{
  "name": "aliyun-pds-js-sdk",
  "version": "0.2.0",
  "description": "PDS SDK for javascript",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.browser.js",
  "types": "lib/cjs/index.browser.d.ts",
  "browser": "lib/cjs/index.browser.js",
  "scripts": {
    "prepare": "husky install",
    "prepublish": "npm run build:lib && npm run build:dist",
    "start": "tsnd --respawn --transpile-only src/index.ts",
    "build:lib": "tsc --build tsconfig-cjs.json && tsc --build tsconfig-esm.json",
    "build:dist": "browserify src/index.browser.ts -p [ tsify  --declaration ] -s PDS_SDK -o  ./dist/aliyun-pds-js-sdk.js && uglifyjs  ./dist/aliyun-pds-js-sdk.js -o  ./dist/aliyun-pds-js-sdk.min.js",
    "test:ft": "mocha -r espower-typescript/guess --reporter spec tests/ft/index.js tests/ft/*.test.{js,mjs,ts}",
    "test:ut": "mocha -r espower-typescript/guess --reporter spec tests/ut/index.ts tests/ut/*.test.{js,mjs,ts}",
    "pretest:web": "rm -rf dist && npm run build:dist && ts-node tests/web/gen-token.js",
    "test:web": "npm-run-all --parallel test:web_sv test:web_open",
    "test:web_sv": "static .",
    "test:web_open": "open http://localhost:8080/tests/web/",
    "test:ts": "mocha -r espower-typescript/guess tests/ut/index.ts",
    "format": "prettier --write 'src/**/*.{js,mjs,ts}' 'tests/**/*.{js,mjs,ts}' 'examples/**/*.{js,mjs,ts}'",
    "lint": "eslint src --fix --ext .ts,.tsx ",
    "cov": "npm run build:lib && npm run build:dist && nyc --reporter=lcov mocha -r espower-typescript/guess tests/ut/index.ts tests/ut/*.test.{ts,mjs,js}  tests/ft/*.test.{ts,mjs,js}",
    "ci": "npm run lint && npm run cov"
  },
  "directories": {
    "test": "tests/"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "tests/**",
      "lib/**",
      "src/utils/crc64/wasm/crc64-wasm.js",
      "src/utils/sha1/wasm/sha1-wasm.js",
      "src/utils/axios-node-adapter.js"
    ],
    "extension": [
      ".js",
      ".ts"
    ]
  },
  "author": "guangchun.luo@alibaba-inc.com",
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "browserify": "^17.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "espower-typescript": "^10.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "power-assert": "^1.6.1",
    "prettier": "^2.4.1",
    "sinon": "^12.0.1",
    "tsify": "^5.0.4",
    "typescript": "^4.4.4",
    "uglify-js": "^3.15.0"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.8",
    "axios": "^0.24.0",
    "debug": "^4.3.2",
    "js-sha1": "^0.6.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
